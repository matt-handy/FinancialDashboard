<script type="text/javascript">
$(function () {
    $('#container<%= @next_plot.id %>').highcharts({
        title: {
            text: '<%= @next_plot.name %>',
            x: -20 //center
        },
            xAxis: {
                type: 'datetime'
            },
            yAxis: {
                title: {
                    text: 'Value'
                }
            },
        legend: {
            layout: 'vertical',
            align: 'right',
            verticalAlign: 'middle',
            borderWidth: 0
        },
        series: [

<%= @next_plot.dataset.html_safe %>
        ]
    });
});
</script>

<div id="container<%= @next_plot.id %>" style="width: 800px; height: 400px; margin: 0 auto"></div>
Links to Google Finance:
<% @next_plot.indicators.each{ |i| %>
  <a href="https://www.google.com/finance?cid=<%= i.uri %>"><%= i.name %></a>
<% } %>
